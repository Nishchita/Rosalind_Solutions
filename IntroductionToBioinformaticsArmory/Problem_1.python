def count_dna_nucleotides(dna_sequence):
	print dna_sequence
	count_array=[0 for x in xrange(4)]
	for base_pair in dna_sequence:
		if base_pair=='A':
			count_array[0] +=1
		elif base_pair=='C':
			count_array[1] +=1
		elif base_pair=='G':
			count_array[2] +=1
		elif base_pair=='T':
			count_array[3] +=1
	return count_array

result = count_dna_nucleotides("AGCTTTTCATTCTGACTGCAACGGGCAATATGTCTCTGTGTGGATTAAAAAAAGAGTGTCTGATAGCAGC")
print(result)